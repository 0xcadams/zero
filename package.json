{
  "name": "reps",
  "description": "Multiplayer server for Replicache",
  "version": "0.1.0",
  "repository": "github:rocicorp/reps-do",
  "type": "module",
  "scripts": {
    "format": "prettier --write 'src/**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check '**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "build": "node build.js",
    "dev": "miniflare --live-reload --debug",
    "test": "npm run build && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "types:check": "tsc && tsc -p test/tsconfig.json"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.18",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "jest-environment-miniflare": "^2.2.0",
    "miniflare": "^2.2.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "zod": "^3.11.6"
  },
  "dependencies": {
    "replicache": "^9.0.0-beta.0"
  },
  "main": "out/reps.js",
  "types": "out/reps.d.ts",
  "exports": {
    ".": {
      "module": "./out/reps.mjs",
      "require": "./out/reps.js",
      "default": "./out/reps.mjs"
    },
    "./out/client": {
      "module": "./out/client.mjs",
      "require": "./out/client.js",
      "default": "./out/client.mjs"
    }
  },
  "files": [
    "out/reps.d.ts",
    "out/reps.js",
    "out/reps.mjs",
    "out/client.d.ts",
    "out/client.js",
    "out/client.mjs"
  ]
}
