{
  "name": "reflect.net",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-next": "./keepalive.sh NODE_ENV=development next dev",
    "dev-worker": "echo 'local mode is currently broken, use dev-worker-prod' && exit 1 && npm run dev-worker-prod -- --local --persist",
    "dev-worker-staging": "./keepalive.sh npm run wrangler-staging -- dev",
    "dev-worker-prod": "./keepalive.sh npm run wrangler-prod -- dev",
    "reset-data-staging": "./write-secret.sh CLEAN_ROOM_UID $RANDOM staging",
    "reset-data-prod": "./write-secret.sh CLEAN_ROOM_UID $RANDOM",
    "build-wasm": "./renderer/build.sh --target web --release",
    "build-wasm-debug": "./renderer/build.sh --target web --debug",
    "nuke-it-from-orbit": "rm -rf node_modules package-lock.json .wrangler .next .vercel; npm i",
    "build": "next build",
    "lint": "next lint",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}'",
    "check-types-js": "tsc -p tsconfig.json --noEmit && tsc -p demo/worker/tsconfig.json --noEmit",
    "check-types": "npm run build-wasm && npm run check-types-js",
    "test": "mocha",
    "test-wasm": "cd renderer && wasm-pack test --chrome --headless",
    "publish-worker-staging": "npm run wrangler-staging -- publish",
    "publish-worker-prod": "npm run wrangler-prod -- publish",
    "worker-logs-staging": "npm run wrangler-staging -- tail",
    "worker-logs-prod": "npm run wrangler-prod -- tail",
    "wrangler-staging": "npx wrangler --config wrangler-staging.toml",
    "wrangler-prod": "npx wrangler --config wrangler.toml",
    "update-reflect": "./build-latest-reflect.sh",
    "update-reflect-pull": "./build-latest-reflect.sh pull",
    "prepare": "husky install"
  },
  "dependencies": {
    "@rocicorp/logger": "^2.2.0",
    "@rocicorp/reflect": "file:vendor/rocicorp-reflect-0.13.0-beta.0-6044588a80f28dfde8a508aa7d8c0d999ccd3e76.tgz",
    "@rocicorp/reflect-server": "file:vendor/rocicorp-reflect-server-0.22.0-beta.0-6044588a80f28dfde8a508aa7d8c0d999ccd3e76.tgz",
    "base-64": "^1.0.0",
    "nanoid": "^3.3.4",
    "react-range": "^1.8.14",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@babylonjs/core": "5.46.0",
    "@babylonjs/inspector": "^5.46.0",
    "@babylonjs/loaders": "5.46.0",
    "@cloudflare/workers-types": "^3.11.0",
    "@datadog/browser-logs": "^4.11.3",
    "@next/font": "13.1.5",
    "@rocicorp/eslint-config": "^0.2.0",
    "@rocicorp/prettier-config": "^0.1.1",
    "@rocicorp/resolver": "^1.0.0",
    "@types/base-64": "^1.0.0",
    "@types/mocha": "^10.0.1",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-scroll": "^1.8.6",
    "@types/react-slider": "^1.3.1",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/styled-components": "^5.1.26",
    "@vercel/node": "^2.8.13",
    "buffer": "^5.7.1",
    "classnames": "^2.3.2",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "husky": "^8.0.3",
    "mocha": "^10.1.0",
    "next": "13.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scroll": "^1.8.9",
    "styled-components": "^5.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vercel": "^28.12.7"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
